<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mustfaibra.roffu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.mustfaibra.roffu.MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/shoe_haven"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shoe_haven"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppTemplate"
        tools:targetApi="31">
        <profileable
            android:shell="true"
            tools:targetApi="q" />

        <activity
            android:name="com.mustfaibra.roffu.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppTemplate">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>